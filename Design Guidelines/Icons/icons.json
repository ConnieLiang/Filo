  "stateVariants": {
    "description": "Icons with before/after or on/off states",
    "pairs": [
      { "inactive": "inbox-before", "active": "inbox-after" },
      { "inactive": "star-before", "active": "star-after" },
      { "inactive": "thumbsup-before", "active": "thumbsup-after" },
      { "inactive": "thumbsdown-before", "active": "thumbsdown-after" },
      { "inactive": "todo", "active": "todo-off" },
      { "inactive": "notification-on", "active": "notification-off" },
      { "inactive": "checkbox-unchecked", "active": "checkbox-checked" },
